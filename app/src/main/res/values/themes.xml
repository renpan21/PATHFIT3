<resources xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="Base.Theme.PATHFIT3" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/pixelBlue</item>
        <item name="colorOnPrimary">@color/skyBlue</item>
        <item name="colorPrimaryVariant">@color/cloudBurst</item>


        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="screenBackGround">
        <item name="android:background">@color/skyBlue</item>
    </style>
    <style name="cardBackGround">
        <item name="android:backgroundTint">@color/white</item>
    </style>
    <style name="textColorTeal">
        <item name="android:textColor">@color/teal_200</item>
    </style>
    <style name="textColorLeafy">
        <item name="android:textColor">@color/leafyGreen</item>
    </style>
    <style name="textColorWhite">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="textColor">
        <item name="android:textColor">@color/blackRock</item>
    </style>
    <style name="textColorCloud">
        <item name="android:textColor">@color/cloudBurst</item>
    </style>
    <style name="buttonColor"><item name="android:backgroundTint">@color/white</item></style>
    <style name="imageColor">
        <item name="android:backgroundTint">@color/blackRock</item>
    </style>



    <style name="Theme.PATHFIT3" parent="Base.Theme.PATHFIT3" />
</resources>